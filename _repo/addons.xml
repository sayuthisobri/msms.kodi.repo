<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.msms" name="Malay Movies and Series" version="1.0.0" provider-name="MSMS">
    <requires>
        <import addon="xbmc.python" version="2.1.0" />
        <import addon="script.module.urlresolver" version="3.0.21" />
        <import addon="script.module.t0mm0.common" version="2.1.1" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Malaysian Movies, Tv Series, Tv Show, Drama</summary>
        <description>Malaysian Movies, Tv Series, Tv Show, Drama</description>
        <disclaimer lang="en">admin@msms.cf</disclaimer>
        <platform>all</platform>
    </extension>
</addon>

    <addon id="repository.msms" name="MSMS repo" version="0.1.0" provider-name="MSMS">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="MSMS repo">
            <info compressed="false">https://raw.githubusercontent.com/sayuthisobri/msms.kodi.repo/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/sayuthisobri/msms.kodi.repo/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/sayuthisobri/msms.kodi.repo/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>www.msms.cf</summary>
            <description>MSMS repo.</description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
